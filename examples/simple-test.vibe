# Simple test
let main = fn () ->
  print "Testing builtin functions"
  
  # Test that test function exists
  let result = test "simple test" (fn () -> 42)
  print "Test function returned"
  
  # Test assert
  assert true "This should not fail"
  print "Assert passed"
  
  # Test inspect
  let x = inspect 42 "The answer"
  print "Inspect returned"
  
main ()